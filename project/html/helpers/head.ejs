<meta charset="UTF-8" />
<title>Base Web App</title>

<!-- First we import our css files -->

<!-- bootstrap css -->
<link
  rel="stylesheet"
  type="text/css"
  href="//maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
/>

<!-- Google font CSS -->
<link
  href="https://fonts.googleapis.com/css?family=Poppins:400,700"
  rel="stylesheet"
/>

